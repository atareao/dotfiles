{
    "inline code": {
        "prefix": "ic",
        "body": ["`$1`$0"]
    },
    "code block": {
        "prefix": "cb",
        "body": [
            "```",
            "$1",
            "```",
            "$0"
        ]
    },
    "zettel note": {
        "prefix": "zn",
        "body": [
            "---",
            "title: $1",
            "filename: $TM_FILENAME",
            "created_at: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "updated_at: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "tags: $2",
            "---",
            "$0"
        ]
    },
    "zettel podcast": {
        "prefix": "zp",
        "body": [
            "---",
            "season: $1",
            "episode: $2",
            "title: $3",
            "filename: $TM_FILENAME",
            "created_at: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "updated_at: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "tags: $4",
            "---",
            "$0"
        ]
    },
    "zettel chapter": {
        "prefix": "zc",
        "body": [
            "---",
            "title: $1",
            "tutorial: $2",
            "filename: $TM_FILENAME",
            "created_at: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "updated_at: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "tags: $3",
            "---",
            "$0"
        ]
    }
}
