{
    "date": {
        "prefix": "date",
        "body": [
            "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $0"
        ]
    },
    "inline code": {
        "prefix": "ic",
        "body": [
            "`$1`$0"
        ]
    },
    "code block python": {
        "prefix": "cp",
        "body": [
            "```python",
            "$1```",
            "$0"
        ]
    },
    "code block bash": {
        "prefix": "cb",
        "body": [
            "```bash",
            "$1```",
            "$0"
        ]
    },
    "zettel note": {
        "prefix": "zn",
        "body": [
            "---",
            "title: $1",
            "created_at: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "tags: $2",
            "---",
            "[Índice](../indice.md)",
            "",
            "# $1",
            "",
            "$0",
            "",
            "<!--more-->",
            "## $1",
            "",
            "---",
            "Mas información,",
            ""
        ]
    },
    "zettel indice": {
        "prefix": "zi",
        "body": [
            "* [[indice|indice]]",
            "tags: $1",
            "---",
            "# Indice",
            "",
            "* [] $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE | $2",
            "---",
            "Mas información,",
            ""
        ]
    },
